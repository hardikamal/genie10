<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgress">
        <attr name="bubbleColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="TypefacedTextView">
        <attr name="typeface" format="string" />
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="lv_title" format="string" />
        <attr name="lv_loading" format="boolean" />
        <attr name="lv_color" format="color" />
    </declare-styleable>

    <declare-styleable name="GifMoviewView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifMoviewViewStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="CircularButton">
        <attr name="shadowColor" format="color" />
        <attr name="buttonColor" format="color" />
    </declare-styleable>
</resources>